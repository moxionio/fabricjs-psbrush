{
  "name": "fabricjs-psbrush",
  "version": "0.0.17",
  "description": "A lightweight pressure-sensitive brush implementation for Fabric.js",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "prepare": "rollup -c",
    "watch": "rollup -cw",
    "clean": "del-cli dist",
    "prettify": "prettier --write ./**/*.{ts,tsx}",
    "dev-pages": "next ./docs",
    "build-pages": "next build ./docs",
    "start-pages": "next start ./docs",
    "export-pages": "next export ./docs",
    "build-docs": "typedoc --mode library --excludeExternals --excludeNotExported --excludePrivate --excludeProtected --out ./docs/out/api ./lib/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moxionio/fabricjs-psbrush.git"
  },
  "keywords": [
    "canvas",
    "graphic",
    "graphics",
    "SVG",
    "node-canvas",
    "fabric"
  ],
  "author": "Jun Kato <i@junkato.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arch-inc/fabricjs-psbrush/issues"
  },
  "homepage": "https://github.com/arch-inc/fabricjs-psbrush#readme",
  "dependencies": {
    "fabric": "^5.2.1-browser"
  },
  "devDependencies": {
    "@types/fabric": "^4.5.8",
    "@types/react": "^18.0.14",
    "del-cli": "^4.0.1",
    "next": "^12.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "sharp": "^0.30.7",
    "typedoc": "^0.23.2",
    "typescript": "^4.7.4"
  }
}
